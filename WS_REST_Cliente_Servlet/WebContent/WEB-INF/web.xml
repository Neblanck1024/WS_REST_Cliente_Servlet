  <servlet-mapping>
    <servlet-name>ServletUbigeo</servlet-name>
    <url-pattern>/ServletUbigeo</url-pattern>
  </servlet-mapping>
  
  <error-page>
  	<error-code>404</error-code>
  	<location>/error.jsp</location>
  </error-page>
</web-app>