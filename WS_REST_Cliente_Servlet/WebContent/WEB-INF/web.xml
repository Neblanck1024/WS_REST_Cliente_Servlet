  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletCiudadano</servlet-name>
    <url-pattern>/ServletCiudadano</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletSintoma</servlet-name>
